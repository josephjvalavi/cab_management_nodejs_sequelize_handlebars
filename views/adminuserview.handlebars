<div>
<h2 class="text-center ">Cab User Details</h2>
</div>
<div>
    <table class="table table-dark table-hover table-striped mt-4">
    <tr>
        <th>id</th>
        <th>firstname</th>
        <th>secondname</th>
        <th>email</th>
        <th>phone</th>
        <th>password</th>
        <th>Action</th>
    </tr>
    
    {{#each data}}
    <tr>
    <td>{{this.dataValues.id}}</td>
    <td>{{this.dataValues.firstName}}</td>
    <td>{{this.dataValues.lastName}}</td>
    <td>{{this.dataValues.email}}</td>
    <td>{{this.dataValues.phone}}</td>
    <td>{{this.dataValues.password}}</td>
    <td>
        <a href="/adminupdateuser/ {{this.dataValues.id}}" class="text-primary">Update</a>
                    <a href="/admindeleteuser/{{this.dataValues.id}}" class="text-danger"
                        onclick="return confirm('Are you sure you want to delete this')">Delete</a>
    </td>
    {{/each }}
    </tr>
    </table>
</div>